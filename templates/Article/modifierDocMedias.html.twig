{# empty Twig template #}
<form action=" {{ path("utb_admin_modifmediasarticle", {'id': id,'idmedia': idmedia,'locale':locale,'type':type,'genre':2,'typemedia':2}) }} " method="post" {{ form_enctype(form) }}>

	<div class="FrmAjoutArt">
		<label>{{ form_label(form.nomMedia, 'admin.nom'|trans({}, 'messages')) }}</label>
		{{ form_widget(form.nomMedia) }}      
		<div class="clr"></div>
	</div>
		  
	<div class="FrmAjoutArt">
		<label>{{ form_label(form.file,  'admin.doctitre'|trans({}, 'messages')) }}</label>
		{{ form_widget(form.file) }}
		<img src="{{ asset('img/wait.gif') }}" id="attendre_img" style="display:none;"/>
                <div class="clr"></div>
	</div>
    
	<div class="FrmAjoutArt">
		<label>{{ form_label(form.natureDoc, 'admin.nature'|trans({}, 'messages')) }}</label>
		{{ form_widget(form.natureDoc) }}
		<img src="{{ asset('img/wait.gif') }}" id="attendre_img" style="display:none;"/>
                <div class="clr"></div>
	</div>    
	
	<input type="submit" value="{{ 'admin.ajouter'|trans({}, 'messages')}}" name="op" id="j_bouton" >
	<div class="clr"></div>
</form> 


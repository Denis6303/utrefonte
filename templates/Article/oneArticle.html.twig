      function gererAll( articlesId,etat){
         
           if( articlesId.length == 0 ){              
              $('#process_tpl').html($('#sivide').html());
              
            if (etat == 2) {   
                var args = {modal: true, autoOpen: false, title: ($('#titreconfirmsubone').html())};
            }else if (etat == 3){
                var args = {modal: true, autoOpen: false, title: ($('#titreconfirmvalone').html())};
            }else if (etat == 4){
                var args = {modal: true, autoOpen: false, title: ($('#titreconfirmpubone').html())};
            }else if (etat == 5){
                var args = {modal: true, autoOpen: false, title: ($('#titreconfirmdepubone').html())};
            }                          
         
           $('#process_tpl').dialog(args);
           $('#process_tpl').dialog({
		buttons: { 			                  
			"OK": function() {
                            $(this).dialog("close");
			}
		}
            })    
           $("#process_tpl").dialog('open'); 
              return false;
          }     
          
          
          if (etat == 2) {   
                $('#process_tpl').html($('#msgconfirmsubone').html());
          }else if (etat == 3){
                $('#process_tpl').html($('#msgconfirmvalone').html());
          }else if (etat == 4){
                $('#process_tpl').html($('#msgconfirmpubone').html());
          }else if (etat == 5){
                $('#process_tpl').html($('#msgconfirmdepubone').html());
          }
      
          if (etat == 2) {   
              var args = {modal: true, autoOpen: false, title: ($('#titreconfirmsubone').html())};
          }else if (etat == 3){
              var args = {modal: true, autoOpen: false, title: ($('#titreconfirmvalone').html())};
          }else if (etat == 4){
              var args = {modal: true, autoOpen: false, title: ($('#titreconfirmpubone').html())};
          }else if (etat == 5){
              var args = {modal: true, autoOpen: false, title: ($('#titreconfirmdepubone').html())};
          }        
          
          if (etat == 2) {   
                $('#process_tpl').html($('#msgconfirmsubone').html());
          } 

          if (etat == 3){
                $('#process_tpl').html($('#msgconfirmvalone').html());
          }

          if (etat == 4){
                $('#process_tpl').html($('#msgconfirmpubone').html());
          }

          if (etat == 5){
                $('#process_tpl').html($('#msgconfirmdepubone').html());
          }
          //var args = {modal: true, autoOpen: false, title: ('Changer le statut des articles')};
         
           $('#process_tpl').dialog(args);
           $('#process_tpl').dialog({
		buttons: {
 			"OK": function() {                 

        
                            $('#process_tpl').html(html);

                            $.ajax({
                                type: "POST",
                                url: "{{ path('utb_admin_gererAllarticles')}}",
                                data: {articlesIds:articlesId,etat:etat},
                                cache: false,
                                success: function(data){ 
                            
                                    var answer = jQuery.parseJSON(data);
                                    if(answer.result == "erreurstatut"){
                                        
                                        if (etat == 2) {   
                                              $('#process_tpl').html($('#impossiblesubone').html());
                                              window.location.reload();
                                        }
                                        if (etat == 3){
                                              $('#process_tpl').html($('#impossiblevalone').html());
                                              window.location.reload();
                                        }
                                        if (etat == 4){
                                              $('#process_tpl').html($('#impossiblepubone').html());
                                              window.location.reload();
                                        }
                                        if (etat == 5){
                                              $('#process_tpl').html($('#impossibledepubone').html());
                                              window.location.reload();
                                        }  
                                         
                               
                                    }
                                    else if (answer.result == "success"){
                                                                                
                                        if (etat == 2) {   
                                              $('#process_tpl').html($('#reussitesubone').html());
                                              window.location.reload();
                                        }
                                        
                                        if (etat == 3){
                                              $('#process_tpl').html($('#reussitevalone').html());
                                              window.location.reload();
                                        }
                                        
                                        if (etat == 4){
                                              $('#process_tpl').html($('#reussitepubone').html());
                                              window.location.reload();
                                        }
                                        
                                        if (etat == 5){
                                              $('#process_tpl').html($('#reussitedepubone').html());
                                              window.location.reload();
                                        }     
                                         //window.location.reload();
                                        
                                        
                                    }else{
                                        $('#process_tpl').html($('#accessdenied').html()); 
                                        window.location.reload();
                                    }                       
                            
                            
                                  
                                }
                            });
                            
			},                   
			"Cancel": function() {
                            $(this).dialog("close");
			}
		}
            })    
            $("#process_tpl").dialog('open'); 
          
       }
      
    </script>
            

